{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.ts"
        }
    ],
    "installCommand": "npm install",
    "buildCommand": "npm run build"
}